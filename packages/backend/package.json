    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "webpack": "^5.99.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
